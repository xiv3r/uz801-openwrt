#!/bin/sh /etc/rc.common
START=95
USE_PROCD=1

start_service() {
	procd_open_instance
	procd_set_param command /bin/sh /etc/msm8916-usb-gadget.sh start
	procd_set_param stdout 1
	procd_set_param stderr 1
	procd_close_instance
}

stop_service() {
	/bin/sh /etc/msm8916-usb-gadget.sh stop 2>/dev/null || true
}
